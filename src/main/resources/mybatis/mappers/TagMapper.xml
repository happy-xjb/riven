<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="com.yealink.dao.TagDao">

    <select id="selectByService" resultType="com.yealink.entities.Tag" parameterType="string">
        select * from tag where service_id = #{id}
    </select>
    <select id="selectValueByService" resultType="java.lang.String" parameterType="string">
        select value from tag where service = #{id}
    </select>

</mapper>
